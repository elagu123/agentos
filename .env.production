# AgentOS Production Environment Configuration
# CRITICAL: This file contains production secrets - NEVER commit to git

# Application Settings
APP_NAME="AgentOS"
APP_VERSION="1.0.0"
DEBUG=false
ENVIRONMENT="production"
API_V1_PREFIX="/api/v1"

# Server Settings
HOST="0.0.0.0"
PORT=8000
WORKERS=4

# Database - AWS RDS PostgreSQL with pgvector
DATABASE_URL=postgresql+asyncpg://agentos_prod:SECURE_PASS_HERE@db.agentos.io:5432/agentos_production
DATABASE_ECHO=false
DATABASE_POOL_SIZE=20
DATABASE_MAX_OVERFLOW=30

# Redis - AWS ElastiCache
REDIS_URL=redis://:REDIS_PASS_HERE@redis.agentos.io:6379/0
REDIS_POOL_SIZE=50

# Vector Store - Qdrant Cloud
QDRANT_URL=https://qdrant.agentos.io
QDRANT_API_KEY=your-qdrant-production-api-key
QDRANT_COLLECTION_NAME=business_context_prod

# LLM Providers - Production Keys
OPENAI_API_KEY=sk-prod-xxxxx
ANTHROPIC_API_KEY=sk-ant-prod-xxxxx
TOGETHER_API_KEY=together-prod-xxxxx
GROQ_API_KEY=groq-prod-xxxxx
DEFAULT_LLM_PROVIDER=openai
DEFAULT_MODEL=gpt-4o

# Authentication - Clerk Production
CLERK_SECRET_KEY=sk_live_xxxxx
CLERK_PUBLISHABLE_KEY=pk_live_xxxxx
CLERK_JWT_KEY=clerk-jwt-production-key

# Security Settings
JWT_SECRET=GENERATE_WITH_OPENSSL_RAND_BASE64_32
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
ENCRYPTION_KEY=GENERATE_WITH_FERNET
RATE_LIMIT_REDIS_URL=redis://redis.agentos.io:6380/1

# Storage - AWS S3 / Cloudflare R2
AWS_ACCESS_KEY_ID=AKIA-xxxxx
AWS_SECRET_ACCESS_KEY=xxxxx
S3_BUCKET=agentos-production
AWS_REGION=us-east-1
CLOUDFLARE_R2_ENDPOINT=https://xxxxx.r2.cloudflarestorage.com
CLOUDFLARE_R2_ACCESS_KEY=xxxxx
CLOUDFLARE_R2_SECRET_KEY=xxxxx

# Monitoring & Analytics
SENTRY_DSN=https://xxxxx@sentry.io/xxxxx
SENTRY_ENVIRONMENT=production
DATADOG_API_KEY=xxxxx
DATADOG_APP_KEY=xxxxx
DATADOG_SITE=datadoghq.com

# Payment Processing - Stripe Live
STRIPE_SECRET_KEY=sk_live_xxxxx
STRIPE_PUBLISHABLE_KEY=pk_live_xxxxx
STRIPE_WEBHOOK_SECRET=whsec_xxxxx
STRIPE_PRICE_ID_BASIC=price_xxxxx
STRIPE_PRICE_ID_PRO=price_xxxxx
STRIPE_PRICE_ID_ENTERPRISE=price_xxxxx

# Email Service - Resend
RESEND_API_KEY=re_xxxxx
SMTP_HOST=smtp.resend.com
SMTP_PORT=587
FROM_EMAIL=hello@agentos.io
SUPPORT_EMAIL=support@agentos.io

# WhatsApp Business API
WHATSAPP_ACCESS_TOKEN=production-whatsapp-token
WHATSAPP_PHONE_NUMBER_ID=production-phone-number-id
WHATSAPP_BUSINESS_ACCOUNT_ID=production-business-account-id
WHATSAPP_WEBHOOK_VERIFY_TOKEN=production-webhook-token

# Google Workspace Integration
GOOGLE_CLIENT_ID=production-google-client-id
GOOGLE_CLIENT_SECRET=production-google-client-secret
GOOGLE_REDIRECT_URI=https://api.agentos.io/auth/google/callback

# Domain Configuration
APP_URL=https://app.agentos.io
API_URL=https://api.agentos.io
FRONTEND_URL=https://agentos.io
ALLOWED_ORIGINS=https://agentos.io,https://app.agentos.io,https://admin.agentos.io

# SSL/TLS Configuration
SSL_CERT_PATH=/etc/ssl/certs/agentos.crt
SSL_KEY_PATH=/etc/ssl/private/agentos.key
FORCE_HTTPS=true

# Background Jobs
CELERY_BROKER_URL=redis://redis.agentos.io:6379/2
CELERY_RESULT_BACKEND=redis://redis.agentos.io:6379/3
CELERY_WORKERS=8

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=/var/log/agentos/api.log

# Rate Limiting
RATE_LIMIT_REQUESTS=1000
RATE_LIMIT_WINDOW=60
RATE_LIMIT_STORAGE=redis

# Security Headers
ENABLE_HSTS=true
HSTS_MAX_AGE=31536000
ENABLE_CSP=true
CSP_REPORT_URI=https://api.agentos.io/security/csp-report

# Health Checks
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=10

# Backup Configuration
BACKUP_S3_BUCKET=agentos-backups
BACKUP_RETENTION_DAYS=30
BACKUP_SCHEDULE="0 2 * * *"  # Daily at 2 AM

# Feature Flags
ENABLE_BETA_FEATURES=false
ENABLE_ANALYTICS=true
ENABLE_TELEMETRY=true

# CDN Configuration
CDN_URL=https://cdn.agentos.io
STATIC_URL=https://static.agentos.io

# Social Media API Keys
INSTAGRAM_ACCESS_TOKEN=production-instagram-token
FACEBOOK_ACCESS_TOKEN=production-facebook-token
LINKEDIN_CLIENT_ID=production-linkedin-client-id
LINKEDIN_CLIENT_SECRET=production-linkedin-client-secret
TWITTER_API_KEY=production-twitter-api-key
TWITTER_API_SECRET=production-twitter-api-secret

# Notification Services
SLACK_BOT_TOKEN=xoxb-production-slack-token
SLACK_SIGNING_SECRET=production-slack-signing-secret
DISCORD_BOT_TOKEN=production-discord-bot-token
TWILIO_ACCOUNT_SID=production-twilio-account-sid
TWILIO_AUTH_TOKEN=production-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Push Notifications
FIREBASE_PROJECT_ID=agentos-production
FIREBASE_PRIVATE_KEY_ID=production-firebase-private-key-id
FIREBASE_PRIVATE_KEY=production-firebase-private-key
FIREBASE_CLIENT_EMAIL=firebase-adminsdk@agentos-production.iam.gserviceaccount.com

# API Keys for External Services
NEWSAPI_KEY=production-newsapi-key
SERPAPI_KEY=production-serpapi-key
WEATHERAPI_KEY=production-weatherapi-key

# Kubernetes Secrets (for reference)
K8S_NAMESPACE=production
K8S_CLUSTER=agentos-production
K8S_REGION=us-east-1